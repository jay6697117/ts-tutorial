    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.18.0",
    "eslint": "^8.27.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.7.1",
    "ts-loader": "^9.2.5",
    "typescript": "3.9.2",
    "webpack": "^5.53.0",
